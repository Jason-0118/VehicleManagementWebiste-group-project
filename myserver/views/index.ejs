<%- include('commonHeader.ejs', {title: "Owner Information"}); %>
    <main class="row mt-4">
        <div class="all">
            <hr class="featurette-divider" />
            <h1 id="header1">Choose your vehicle</h1>
            <h2 class="bg-dark text-white text-center">
                Tesla
            </h2>
            <div class="row featurette text-center">
                <div class="col-md-4 order-md-1">
                    <img src="./images/newCar.jpg" alt="This is a image of badminton" class="img-fluid" />
                </div>
                <div class="col-md-8 order-md-2 align-self-center">
                    <div class="right-content">
                        <table id="tablecar">
                            <th><a class="text-decoration-none" href="./owner.html">Owner</a></th>
                            <th><a class="text-decoration-none" href="./status.html">Title Status</a></th>
                            <th><a class="text-decoration-none" href="./insurance.html">Insurance</a></th>
                            </tr>
                            <tr>
                                <td>Make:
